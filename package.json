{
  "name": "@yolominds/seacows-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for Seacows backend",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "format:check": "prettier --check '**/*.{js,ts,json,yaml,yml,md}'",
    "format:write": "prettier --write '**/*.{js,ts,json,yaml,yml,md}'",
    "prepare": "husky install",
    "lint": "eslint --max-warnings 0 '**/*.{js,ts}'"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "husky": "^7.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.7"
  },
  "engines": {
    "node": "18.x",
    "yarn": ">=1.22.11"
  },
  "packageManager": "yarn@1.22.19"
}
